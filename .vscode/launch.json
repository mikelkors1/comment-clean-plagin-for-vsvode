// Корневой объект конфигурации запуска
{
// Версия схемы launch.json, поддерживаемая VS Code
    "version": "0.2.0",
// Список конфигураций, доступных для запуска расширения
    "configurations": [
// Описание основной конфигурации запуска расширения
        {
// Название конфигурации, показываемое в выпадающем списке отладчика
            "name": "Run Extension",
// Тип указывает, что нужно запустить хост расширений VS Code
            "type": "extensionHost",
// Режим запуска — запуск (а не вложение к уже запущенному процессу)
            "request": "launch",
// Путь к исполняемому файлу текущего экземпляра VS Code
            "runtimeExecutable": "${execPath}",
// Аргументы, передаваемые при запуске: путь к каталогу расширения для разработки
            "args": ["--extensionDevelopmentPath=${workspaceFolder}"],
// Пути к выходным файлам для сопоставления исходников при отладке
            "outFiles": ["${workspaceFolder}/out/**/*.js"]
// Завершение описания конфигурации запуска расширения
        }
// Завершение списка конфигураций
    ]
// Завершение корневого объекта launch.json
}